<div class="table-responsive">
  <table class="table table-striped">
      <thead>
          <tr>
              <th></th>
              <th>#</th>
              <th>Title</th>
              <th>Artist</th>
              {{!-- <th>Album</th> --}}
              <th>Duration</th>
          </tr>
      </thead>
      <tbody>
          {{#each displayedTracksList as |track|}}
            {{track-item clickTrack="clickTrack" model=track}}
          {{/each}}
      </tbody>
  </table>
</div>
