<a {{action "clickTrackItem" model}} class="{{if model.isActive 'active'}} list-group-item">
    <i class="fa fa-tasks fa-fw"></i> {{model.name}}
    <span class="pull-right text-muted small" style="background: white;border-radius: 9px;padding: 2px 8px;"><em>{{model.durationFormated}}</em>
    </span>
</a>
